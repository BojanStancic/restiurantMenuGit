<form method="post" action="{{ path('list_of_delete', {'id': list_of.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ list_of.id) }}">
    <button class="btn btn-danger float-right">Delete</button>
</form>
